//maximun and minimum value
#include<stdio.h>
#include<conio.h>
int main()
{
	int max,min,i,n;
	printf("enter the size: ");
	scanf("%d",&n);
	int a[n];
	for(i=0;i<n;i++)
	
	{
		scanf("%d",&a[i]);
	}
	min=a[0];
	max=a[0];
	for(i=1;i<n;i++)
	{
		if(a[i]<min)
		{
	
		min=a[i];
		}
	    if(a[i]>max)
	    {
	    max=a[i];
		}
	}
	    	printf("%d\n",min);
	    	printf("%d\n",max);
	}
